{
  "name": "aymfxui",
  "version": "0.0.1",
  "description": "组件库",
  "private": true,
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm --filter play run dev",
    "build": "vue-tsc --noEmit && vite build",
    "publish": "pnpm run -C dist publish"
  },
  "author": "ly",
  "dependencies": {
    "@aymfx-ui/components": "workspace:*",
    "@aymfx-ui/hooks": "workspace:*",
    "@aymfx-ui/utils": "workspace:*",
    "element-plus": "^2.4.4",
    "vue": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vikingmute/v-element": "^0.0.4",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "release-it": "^17.0.1",
    "sass": "^1.69.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0"
  }
}
